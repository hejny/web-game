{
  "name": "web-game",
  "version": "0.6.1",
  "homepage": "https://hejny.github.io/web-game/",
  "private": true,
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prettier": "prettier --config .prettierrc --write \"src/**/*.{ts,tsx,css}\" webpack.config.js",
    "prettier-watch": "onchange --initial \"**/*.{ts,tsx,css}\" -- npm run prettier"
  },
  "dependencies": {
    "@types/lodash": "^4.14.71",
    "@types/react": "^15.0.38",
    "babylonjs": "^3.0.1-alpha",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.5.0",
    "react-redux": "^5.0.5",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "superagent": "^3.5.2",
    "superagent-promise": "^1.1.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "react-scripts-ts": "^2.4.0",
    "onchange": "^5.0.2",
    "prettier": "^1.13.7"
  }
